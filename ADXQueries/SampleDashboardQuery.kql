opcua_telemetry
| where ExpandedNodeID == "http://opcfoundation.org/UA/Station/#i=406"
| where DataSetWriterID has "assembly.munich"
| where SourceTimestamp > _startTime and SourceTimestamp < _endTime
| order by SourceTimestamp desc
| extend var = todouble(Value)
| project SourceTimestamp, var
| render timechart
