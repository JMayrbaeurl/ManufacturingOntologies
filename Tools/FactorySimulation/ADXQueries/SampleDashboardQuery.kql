let k = adt_dh_ontdemo_ADT_eastus2
| where TimeStamp > _startTime and TimeStamp < _endTime
| where Id == toscalar(GetDigitalTwinIdForUANode("assembly", "munich", "ProductSerialNumber"));
k
| where isnotnull(SourceTimeStamp)
| join k on $left.TimeStamp == $right.TimeStamp
| where Key1 == "OPCUADisplayName"
| project SourceTimeStamp, OPCUANodeValue = toint(Value), OPCUADisplayName = Value1